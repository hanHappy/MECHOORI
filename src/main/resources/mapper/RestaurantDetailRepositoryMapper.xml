<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mechoori.web.repository.RestaurantDetailRepository">

	<select id="findById" resultType="RestaurantDetail">
		select * 
		from restaurant_detail
		where id = #{restaurantId}
	</select>
</mapper>