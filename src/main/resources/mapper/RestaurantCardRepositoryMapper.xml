<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mechoori.web.repository.RestaurantCardRepository">
	<select id="findAllByCtgId" resultType="RestaurantCard">
		select *
		from restaurant_card
		where category_id = #{categoryId}
	</select>
	<select id="findAllByQuery" resultType="RestaurantCard">
		select *
		from restaurant_card
		where name like '%${query}%'
	</select>
	

</mapper>